<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>togglejs@quirk.es</em:id>
        <em:type>2</em:type>
        <em:name>Toggle JavaScript Enabled</em:name>
        <em:version>1.4</em:version>
        <em:bootstrap>true</em:bootstrap>
        <em:description>Toggle whether JavaScript is enabled globally or not. Adds a menu entry and an icon to the URL/title bar.</em:description>
        <em:creator>Richard Quirk</em:creator>
        <em:localized>
          <Description>
            <em:locale>es-ES</em:locale>
            <em:name>Alternar la Activación de JavaScript</em:name>
            <em:description>Activar o desactivar JavaScript de forma global en el navegador. Añade una opción al menú y un icono en la barra del titulo/URL.</em:description>
          </Description>
        </em:localized>
        <em:localized>
          <Description>
            <em:locale>de</em:locale>
            <em:name>Ändern Sie JavaScript aktivieren</em:name>
            <em:description>Aktivieren oder Deaktivieren von JavaScript global im Browser. Fügt eine Option in das Menü und ein Symbol in der Titelzeile / URL</em:description>
          </Description>
        </em:localized>

        <!-- Mobile Native -->
        <em:targetApplication>
            <Description>
                <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
                <em:minVersion>34.0</em:minVersion>
                <em:maxVersion>55.0</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
